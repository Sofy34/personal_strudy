    "import re\n",
    "def get_client(x):\n",
    "    text=docs_dict[x]\n",
    "    num=re.search(r'[0-9]{1,}',text).group(0)\n",
    "    name=re.sub(num,'',text)\n",
    "    return name\n",
    "def get_num(x):\n",
    "    text=docs_dict[x]\n",
    "    num=re.search(r'[0-9]{1,}',text).group(0)\n",
    "    return int(num)"
   ]
  },
